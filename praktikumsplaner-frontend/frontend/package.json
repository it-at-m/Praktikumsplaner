{
    "name": "praktikumsplaner-frontend-frontend",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "security": "vite --mode developmentSecurity",
        "serve": "vite ",
        "build": "vite build && eslint . --fix && vue-tsc --noEmit",
        "test:unit": "vitest run",
        "preview": "vite preview",
        "lint": "prettier . --check && eslint . && vue-tsc --noEmit --skipLibCheck",
        "fix": "prettier . --write && eslint . --fix",
        "type-check": "vue-tsc --noEmit"
    },
    "dependencies": {
        "@mdi/font": "7.3.67",
        "mitt": "^3.0.1",
        "moment": "^2.29.4",
        "pinia": "^2.1.7",
        "roboto-fontface": "^0.10.0",
        "vue": "^3.3.7",
        "vue-router": "^4.2.5",
        "vuetify": "^3.7.2"
    },
    "devDependencies": {
        "@babel/types": "^7.23.0",
        "@ianvs/prettier-plugin-sort-imports": "^4.1.1",
        "@types/jsdom": "^21.1.4",
        "@types/node": "^18.18.8",
        "@vitejs/plugin-vue": "^4.4.0",
        "@vue/eslint-config-prettier": "^8.0.0",
        "@vue/eslint-config-typescript": "^12.0.0",
        "@vue/test-utils": "^2.4.1",
        "eslint": "^8.52.0",
        "eslint-plugin-vue": "^9.18.1",
        "eslint-plugin-vuetify": "^2.1.0",
        "jsdom": "^22.1.0",
        "prettier": "^3.0.3",
        "typescript": "^5.2.2",
        "unplugin-fonts": "^1.0.3",
        "vite": "^4.5.3",
        "vite-plugin-vuetify": "^1.0.2",
        "vitest": "^0.34.6",
        "vue-tsc": "^1.0.8"
    }
}
